<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <PackageId>Kuantik.XmlModels</PackageId>
        <Authors>Diego Arellano Moreno</Authors>
        <Description>XML models for CFDI</Description>
        <PackageTags>XML models Cfdi comprobante 4.0 retenciones validacion formateo</PackageTags>
        <PackageProjectUrl>https://github.com/SaulEnriqueMR-at-Kuantik/dotnet-sat-xml-serializer.git</PackageProjectUrl>
        <RepositoryUrl>https://github.com/SaulEnriqueMR-at-Kuantik/dotnet-sat-xml-serializer.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <Folder Include="App\Interfaces\" />
      <Folder Include="Shared\XmlProcessing\Validator\Interface\" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="13.0.4" />
    </ItemGroup>

</Project>
